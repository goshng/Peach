@ARTICLE{Rosenberg2005,
  author = {Noah A Rosenberg and Saurabh Mahajan and Sohini Ramachandran and
	Chengfeng Zhao and Jonathan K Pritchard and Marcus W Feldman},
  title = {Clines, clusters, and the effect of study design on the inference
