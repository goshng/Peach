  journal = {J Virol},
  year = {1999},
  volume = {73},
  pages = {10489-10502},
  number = {12},
  abstract = {To understand the high variability of the asymptomatic interval between
	primary human immunodeficiency virus type 1 (HIV-1) infection and
	the development of AIDS, we studied the evolution of the C2-V5 region
	of the HIV-1 env gene and of T-cell subsets in nine men with a moderate
	or slow rate of disease progression. They were monitored from the
